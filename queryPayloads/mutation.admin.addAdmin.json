{
    "operationName": "adminAddAdmin",
    "query": "mutation adminAddAdmin ( $addAdminInput:AddAdminInput! $accountId:ID! ) {\n\tadmin ( accountId:$accountId ) {\n\t\taddAdmin ( input:$addAdminInput  )  {\n\t\t\tadminID\n\t\t}\n\n\t}\t\n}",
    "variables": {
        "addAdminInput": {
            "adminType": "LOGIN",
            "email": "string",
            "firstName": "string",
            "lastName": "string",
            "passwordNeverExpires": true,
            "updateAdminRoleInput": {
                "allowedAccounts": [
                    "id1",
                    "id2"
                ],
                "allowedEntities": {
                    "id": "id",
                    "name": "string",
                    "type": "account"
                },
                "role": {
                    "id": "id",
                    "name": "string"
                }
            }
        }
    }
}