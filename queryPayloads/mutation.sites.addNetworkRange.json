{
    "operationName": "sitesAddNetworkRange",
    "query": "mutation sitesAddNetworkRange ( $lanSocketInterfaceId:ID! $addNetworkRangeInput:AddNetworkRangeInput! $accountId:ID! ) {\n\tsites ( accountId:$accountId ) {\n\t\taddNetworkRange ( lanSocketInterfaceId:$lanSocketInterfaceId  input:$addNetworkRangeInput  )  {\n\t\t\tnetworkRangeId\n\t\t}\n\n\t}\t\n}",
    "variables": {
        "addNetworkRangeInput": {
            "azureFloatingIp": "example_value",
            "gateway": "example_value",
            "internetOnly": true,
            "localIp": "example_value",
            "mdnsReflector": true,
            "name": "string",
            "networkDhcpSettingsInput": {
                "dhcpMicrosegmentation": true,
                "dhcpType": "DHCP_RELAY",
                "ipRange": "example_value",
                "relayGroupId": "id"
            },
            "rangeType": "Routed",
            "subnet": "example_value",
            "translatedSubnet": "example_value",
            "vlan": 1
        },
        "lanSocketInterfaceId": "id"
    }
}