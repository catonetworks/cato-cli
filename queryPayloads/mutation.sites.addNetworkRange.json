            },
            "localIp": {
                "localIp": "IPAddress"
            },
            "mdnsReflector": {
                "mdnsReflector": "Boolean"
            },
            "name": {
                "name": "String"
            },
            "networkDhcpSettingsInput": {
                "dhcpType": {
                    "dhcpType": "enum(DhcpType)"
                },
                "ipRange": {
                    "ipRange": "IPRange"
                },
                "relayGroupId": {
                    "relayGroupId": "ID"
                }
            },
