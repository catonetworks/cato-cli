{
    "operationName": "hardwareManagement",
    "query": "query hardwareManagement ( $socketInventoryInput:SocketInventoryInput ) {\n\thardwareManagement ( ) {\n\t\tsocketInventory\n\t}\t\n}",
    "variables": {
        "socketInventoryInput": {
            "pagingInput": {
                "from": 1,
                "limit": 1
            },
            "socketInventoryFilterInput": {
                "freeText": {
                    "search": "string"
                }
            },
            "socketInventoryOrderInput": {
                "accountName": {
                    "direction": "ASC",
                    "priority": 1
                },
                "deliverySiteName": {
                    "direction": "ASC",
                    "priority": 1
                },
                "description": {
                    "direction": "ASC",
                    "priority": 1
                },
                "installedSite": {
                    "direction": "ASC",
                    "priority": 1
                },
                "serialNumber": {
                    "direction": "ASC",
                    "priority": 1
                },
                "shippingCompany": {
                    "direction": "ASC",
                    "priority": 1
                },
                "shippingDate": {
                    "direction": "ASC",
                    "priority": 1
                },
                "socketType": {
                    "direction": "ASC",
                    "priority": 1
                },
                "status": {
                    "direction": "ASC",
                    "priority": 1
                }
            }
        }
    }
}